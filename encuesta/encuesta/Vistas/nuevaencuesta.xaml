<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="encuesta.Vistas.nuevaencuesta">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">

            <!--Nueva Encuesta TIPO WOS -->
            <Button Text="WOS"/>

            <!--Nueva Encuesta TIPO COACHING -->
            <Button Text="COACHING"/>

            <!--Nueva Encuesta TIPO COACHING WOS -->
            <Button Text="PLANILLA DE COACHING WOS"/>

            <!--Nueva Encuesta DINAMINA DE LLENADO DE PLANILLA -->
            <Button Text="DINAMICA DE LLENADO DE LA PLANILLA"/>

            <!-- Clientes -->
            <Button x:Name="BtnClientes" Text="Clientes" Clicked="BtnClientes_OnClick"/>

            <!-- Clientes -->
            <Button x:Name="BtnBorrarDatos" Text="RESETEAR DATOS" Clicked="BtnBorrarDatos_OnClick" BackgroundColor="Red" TextColor="White"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>